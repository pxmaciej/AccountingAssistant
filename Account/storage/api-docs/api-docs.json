                                    },
                                    "seller": {
                                        "type": "string",
                                        "example": "Mechanik Zbysio"
                                    },
                                    "nip": {
                                        "type": "integer",
                                        "format": "int64",
                                        "example": "9999999999"
                                    },
                                    "name": {
                                        "type": "string",
                                        "example": "Silnik v2"
                                    },
                                    "netto": {
                                        "type": "double",
                                        "format": "double",
                                        "example": "400.00"
                                    },
                                    "vat": {
                                        "type": "integer",
                                        "format": "int64",
                                        "example": "23"
                                    },
                                    "brutto": {
                                        "type": "double",
                                        "format": "double",
                                        "example": "492.00"
                                    },
                                    "category": {
                                        "type": "string",
                                        "example": "Serwis"
                                    },
                                    "paid": {
                                        "type": "boolean",
                                        "example": "false"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                },
                "security": [
                    {
                        "apiAuth": []
                    }
                ]
            }
        },
        "/api/expense/destroy/{expense_id}": {
            "delete": {
                "tags": [
